                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:textColor="#000000" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <ImageView
            android:id="@+id/service6"
            android:layout_width="93dp"
            android:layout_height="94dp"
            android:layout_alignParentTop="true"
            android:layout_centerInParent="true"
            android:layout_weight="1"
            android:src="@mipmap/ic_logo_foreground"
            tools:srcCompat="@tools:sample/avatars[0]" />

            <TextView
                android:id="@+id/text6"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:textColor="#000000" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="139dp"
        android:layout_marginTop="28dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/service7"
                android:layout_width="93dp"
                android:layout_height="94dp"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="true"
                android:layout_weight="1"
                android:src="@mipmap/ic_logo_foreground"
                tools:srcCompat="@tools:sample/avatars[15]" />


            <TextView
                android:id="@+id/text7"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:textColor="#000000" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/service8"
                android:layout_width="93dp"
                android:layout_height="94dp"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="true"
                android:layout_weight="1"
                android:src="@mipmap/ic_logo_foreground"
                tools:srcCompat="@tools:sample/avatars[7]" />

            <TextView
                android:id="@+id/text8"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:textColor="#000000" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <ImageView
                android:id="@+id/service9"
                android:layout_width="93dp"
                android:layout_height="94dp"
                android:layout_alignParentTop="true"
                android:layout_centerInParent="true"
                android:layout_weight="1"
                android:src="@mipmap/ic_logo_foreground"
